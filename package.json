{
  "name": "todo-tutorial-youtube",
  "version": "1.0.0",
  "description": "a todo app made with react, graphql, express and mongodb",
  "main": "index.js",
  "engines": {
    "node": "10.2.1",
    "npm": "6.1.0"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "mongoose": "^5.1.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.5"
  },
  "scripts": {
    "server": "nodemon index.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node index.js",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "keywords": [],
  "author": "benjaminadk",
  "license": "ISC"
}
